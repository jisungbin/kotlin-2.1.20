<idea-plugin>
    <id>org.jetbrains.kotlin</id>

    <extensionPoints>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.resolve.jvm.extensions.AnalysisHandlerExtension"
                        qualifiedName="org.jetbrains.kotlin.analyzeCompleteHandlerExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.extensions.internal.CallResolutionInterceptorExtension"
                        qualifiedName="org.jetbrains.kotlin.extensions.internal.callResolutionInterceptorExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.extensions.internal.TypeResolutionInterceptorExtension"
                        qualifiedName="org.jetbrains.kotlin.extensions.internal.typeResolutionInterceptorExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.extensions.TypeAttributeTranslatorExtension"
                        qualifiedName="org.jetbrains.kotlin.extensions.typeAttributeTranslatorExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.resolve.diagnostics.DiagnosticSuppressor" qualifiedName="org.jetbrains.kotlin.diagnosticSuppressor"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.resolve.extensions.SyntheticResolveExtension" qualifiedName="org.jetbrains.kotlin.syntheticResolveExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.resolve.extensions.AssignResolutionAltererExtension" qualifiedName="org.jetbrains.kotlin.assignResolutionAltererExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.backend.common.extensions.IrGenerationExtension" qualifiedName="org.jetbrains.kotlin.irGenerationExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.plugin.references.SimpleNameReferenceExtension" qualifiedName="org.jetbrains.kotlin.simpleNameReferenceExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.codegen.extensions.ClassBuilderInterceptorExtension" qualifiedName="org.jetbrains.kotlin.classBuilderFactoryInterceptorExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.backend.jvm.extensions.ClassGeneratorExtension" qualifiedName="org.jetbrains.kotlin.classGeneratorExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.resolve.jvm.extensions.PackageFragmentProviderExtension" qualifiedName="org.jetbrains.kotlin.packageFragmentProviderExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.extensions.StorageComponentContainerContributor" qualifiedName="org.jetbrains.kotlin.storageComponentContainerContributor"/>
        <extensionPoint area="IDEA_PROJECT"
                        dynamic="true"
                        interface="org.jetbrains.kotlin.resolve.extensions.ExtraImportsProviderExtension" qualifiedName="org.jetbrains.kotlin.extraImportsProviderExtension"/>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.fir.extensions.FirExtensionRegistrarAdapter"
                        qualifiedName="org.jetbrains.kotlin.fir.extensions.firExtensionRegistrar"/>
        <extensionPoint area="IDEA_PROJECT"
                        interface="org.jetbrains.kotlin.serialization.DescriptorSerializerPlugin"
                        qualifiedName="org.jetbrains.kotlin.DescriptorSerializerPlugin"/>
        <extensionPoint interface="org.jetbrains.kotlin.diagnostics.rendering.DefaultErrorMessages.Extension"
                        qualifiedName="org.jetbrains.kotlin.defaultErrorMessages"/>
    </extensionPoints>
</idea-plugin>
